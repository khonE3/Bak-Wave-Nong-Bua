{
  "installCommand": "npm install --ignore-scripts && node node_modules/nuxt/bin/nuxt.mjs prepare",
  "buildCommand": "node node_modules/nuxt/bin/nuxt.mjs generate",
  "outputDirectory": ".output/public",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
